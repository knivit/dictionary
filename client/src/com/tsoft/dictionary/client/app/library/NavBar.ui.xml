<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:d='urn:import:com.tsoft.dictionary.client.app.library'>

  <ui:style>
  .anchor {
    margin: 0 8px;
  }
  </ui:style>

  <g:HTMLPanel>
    <g:Anchor styleName='{style.anchor}' ui:field='newerButton' href='javascript:;'>
      &lt; Prior
    </g:Anchor>

    <span ui:field='countLabel'/>

    <g:Anchor styleName='{style.anchor}' ui:field='olderButton' href='javascript:;'>
      Next &gt;
    </g:Anchor>
  </g:HTMLPanel>
</ui:UiBinder>
