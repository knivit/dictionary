<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:d='urn:import:com.tsoft.dictionary.client.app.dictionary'>

  <g:HTMLPanel>
    <div>
      <b>Dictionary</b>
    </div>

    <table>
      <tr>
        <td>
          <g:ListBox ui:field='dictionaryBox'></g:ListBox>
        </td>

        <td>
          <g:Button text='Refresh List' ui:field='refreshButton'/>
        </td>
      </tr>
    </table>
  </g:HTMLPanel>
</ui:UiBinder>
