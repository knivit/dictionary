<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
    xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:d='urn:import:com.tsoft.dictionary.client.app.dictionary'>

    <g:DockLayoutPanel unit='EM'>
      <g:north size='10'>
        <d:DictionaryPanel ui:field='dictionaryPanel'/>
      </g:north>

      <g:center>
        <d:WordPanel ui:field='wordPanel'/>
      </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>
